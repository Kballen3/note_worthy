<h1>Notes For School </h1>
<%= @notes.each do |note| %>
<h2><%= note.title %></h2>
  <%= link_to 'Show Note', note_path(note.id) %>
  <p><%= link_to 'New Note', new_note_path(note.id) %></p>
  <%= link_to 'Edit page', edit_path_note(note.id)%>
  <%= link_to 'Destroy page', note method: :delete%>
<% end %>




